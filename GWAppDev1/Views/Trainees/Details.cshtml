@model IEnumerable<GWAppDev1.Models.Trainee>
@{
    ViewBag.Title = "Details";
}

<h2>Trainee Details</h2>

<div class="form-group">
    @Html.ActionLink("Back to My Trainees", "Index", null, new { @class = "btn btn-primary" })
</div>
<table class="table table-striped">
    <thread>
        <tr>
            <th class="text-danger">FullName</th>
            <th class="text-danger">Email</th>
            <th class="text-danger">Age</th>
            <th class="text-danger">Address</th>
            <th class="text-danger">DateOfBirth</th>
            <th class="text-danger">Action</th>
        </tr>
    </thread>
    <tbody>
        @foreach (var trainee in Model)
        {
            <tr>
                <td>@trainee.Fullname</td>
                <td>@trainee.Email</td>
                <td>@trainee.Age</td>
                <td>@trainee.Address</td>
                <td>@trainee.DateOfBirth</td>
            </tr>
        }
    </tbody>
</table>